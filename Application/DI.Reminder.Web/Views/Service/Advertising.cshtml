@model IEnumerable<DI.Reminder.BL.ServiceItem>
@{
    Layout = null;
}
@foreach (var item in Model)
{
    <div>
        <a href="@item.Url">
            @Html.DisplayFor(f => item.Title)
        </a>
    </div>
    if (item.Url == null)
    {
        <img style="width:230px; height:240px" ; src="@item.Image" />
    }
    else
    {
        <img style="width:140px; height:200px" ; src="@item.Image" />
    }
}


